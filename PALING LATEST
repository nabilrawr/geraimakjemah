    //CHECK BUTTON ActionEvent
    
    //NASI LEMAK CHECKBOX
    private void chknasilemakActionPerformed(java.awt.event.ActionEvent evt) 
    {
      
        if (chknasilemak.isSelected())
        {
            txtnasilemak.setEnabled(true);
            txtnasilemak.requestFocus();
            txtnasilemak.setText("");
        }
        else
        {
            txtnasilemak.setEnabled(false);
            txtnasilemak.setText("0");
        }
    }
    
