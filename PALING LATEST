 // THIS CODE MAKE TEXTBOX ONLY ACCEPT NUMERIC VALUE USING KeyEvent
    
    private void txtnasilemakKeyTyped(java.awt.event.KeyEvent evt) {
        
        char iNumber = evt.getKeyChar();
        if(!(Character.isDigit(iNumber))
            || (iNumber == KeyEvent.VK_BACK_SPACE)
            || (iNumber == KeyEvent.VK_DELETE)){
        evt.consume();
    }
    }
  
    private void txtroticanaiKeyTyped(java.awt.event.KeyEvent evt) 
    {
       
          char iNumber = evt.getKeyChar();
        if(!(Character.isDigit(iNumber))
            || (iNumber == KeyEvent.VK_BACK_SPACE)
            || (iNumber == KeyEvent.VK_DELETE)){
        evt.consume();
    }
    }

