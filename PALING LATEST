  //BUTTON
  //EXIT BUTTON
    private JFrame frame;
    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) 
    {
      
        frame = new JFrame("EXIT");
        if(JOptionPane.showConfirmDialog(frame,"Comfirm if you want to exit", "NEO CLUB CAFE", 
                JOptionPane.YES_NO_OPTION)== JOptionPane.YES_NO_OPTION){
            System.exit(0);
        }
    }
    
    //RESET BUTTON
    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {
        
    	//will clear receipt
        area.setText("");
       
       //food and drink TXT
        txtnasilemak.setText(null);
        txtroticanai.setText(null);
        txtayampenyet.setText(null);
        txtasampedas.setText(null);
        txtasamboi.setText(null);
        txtairkosong.setText(null);
        txtairsirap.setText(null);
        txtairlimau.setText(null);
        
        //food and drink CHK
        chknasilemak.setSelected(false);
        chkroticanai.setSelected(false);
        chkayampenyet.setSelected(false);
        chkasampedas.setSelected(false);
        chkasamboi.setSelected(false);
        chkairkosong.setSelected(false);
        chkairsirap.setSelected(false);
        chkairlimau.setSelected(false);
       
        
        
    }
   
    
    //GENERATE RECEIPT BUTTON
    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) 
    {
        //calculation happen here
    	try {
    		
        itemcost[0] = Double.parseDouble(txtnasilemak.getText()) * 2.0;
        itemcost[1] = Double.parseDouble(txtroticanai.getText()) * 1.0;
        itemcost[2] = Double.parseDouble(txtayampenyet.getText()) * 5.0;
        itemcost[3] = Double.parseDouble(txtasampedas.getText()) * 7.0;
        itemcost[4] = Double.parseDouble(txtasamboi.getText()) * 2.30;
        itemcost[5] = Double.parseDouble(txtairkosong.getText()) * 0.20;
        itemcost[6] = Double.parseDouble(txtairsirap.getText()) * 1.10;
        itemcost[7] = Double.parseDouble(txtairlimau.getText()) * 4.40;
        
        iSubTotal = itemcost[0] + itemcost[1] + itemcost[2]  + itemcost[3]  
                   + itemcost[4]  + itemcost[5]  + itemcost[6]  + itemcost[7] ;
        
       
        
        String cSubTotal = String.format("RM%.2f", iSubTotal);
        
        iTax = 0.06 * iSubTotal;
        String wtax = String.format("RM%.2f", iTax);
        iTotal = iSubTotal + iTax;       
        String iAmount = String.format("RM%.2f", iTotal);
        
        
        
        String nasilemak = ""; 
        String roticanai = "";
        String ayampenyet = "";
        String asampedas = "";
        String asamboi = "";
        String airkosong = "";
        String airsirap = "";
        String airlimau = "";
        
           if(chknasilemak.isSelected()) { 
        	   nasilemak = "\nNasi Lemak                 " + txtnasilemak.getText() + "         " + " RM 2.00";	
        			} 
           if( chkroticanai.isSelected()) {
        	   roticanai = "\nroticanai                        " + txtroticanai.getText() + "         " + " RM 1.00";	
        			}
           if( chkayampenyet.isSelected()) {
        	   ayampenyet = "\nayampenyet                 " + txtayampenyet.getText() + "         " + " RM 5.00" ;	
        			}
           if(chkasampedas.isSelected()) {
        	   asampedas = "\nasampedas                  " + txtasampedas.getText() + "         " + " RM 7.00";	
        			}
           if(chkasamboi.isSelected()) {
        	   asamboi = "\nasamboi                       " + txtasamboi.getText() + "         " + " RM 2.30";	
        			}
           if(chkairkosong.isSelected()) {
        	   airkosong = "\nairkosong                     " + txtairkosong.getText() + "         " + " RM 0.20";	
        			}
           if(chkairsirap.isSelected()) {
        	   airsirap = "\nairsirap                          " + txtairsirap.getText() + "         " + " RM 1.10";	
        			}
           if(chkairlimau.isSelected()) {
        	   airlimau = "\nairlimau                         " + txtairlimau.getText() + "         " + " RM 4.40";	
        			}
        
    
       
        //receipt generate here

         area.setText("******************************************\n");
         area.setText(area.getText() +"*  WELCOME TO NEO CLUB CAFE  *\n");
         area.setText(area.getText() +"******************************************\n");
         
         Date obj = new Date();
         
         String date = obj.toString();
         area.setText(area.getText() +"\n"+date+"\n\n");
         
         area.setText(area.getText() 
                 +nasilemak
                 +roticanai
                 +ayampenyet
                 +asampedas
                 +asamboi
                 +airkosong
                 +airsirap
                 +airlimau+"\n");
         
         area.setText(area.getText() +"\n"+"GST 6%: "+wtax+"\n");
         area.setText(area.getText() +"SUB TOTAL : "+cSubTotal+"\n");
         area.setText(area.getText() +"TOTAL : "+iAmount+"\n");
         
         area.setText(area.getText() +"******************************************\n");
         area.setText(area.getText() +"*                   THANKYOU                    *\n");
         area.setText(area.getText() +"******************************************\n");
         
    	}
    	
    	//catch arithmeticException
    	catch (ArithmeticException e) {
    		JOptionPane.showMessageDialog(frame,"Exception occured.","ERROR",JOptionPane.WARNING_MESSAGE);      
    		
    	}
    	//general exception
    	catch(Exception e) {
    		JOptionPane.showMessageDialog(frame,"Exception occured please insert a value.","ERROR",JOptionPane.WARNING_MESSAGE);    
    	}
    }
   
    //PRINT BUTTON
    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) 
    {
        
        try
        {
            area.print();
        }
        //general exception
        catch(Exception e)
        { 
        	
        	
        	JOptionPane.showMessageDialog(frame,"Exception occured.","ERROR",JOptionPane.WARNING_MESSAGE);      
              
        }
    }
 
    
